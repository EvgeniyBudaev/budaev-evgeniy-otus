{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "linebreak-style": "off",
    "quotes": "off",
    "require-jsdoc": "off",
    "no-trailing-spaces": "off",
    "max-len": "off",
    "operator-linebreak": "off",
    "comma-spacing": "off",
    "one-var": "off",
    "prefer-const": "off",
    "prefer-rest-params": "off",
    "guard-for-in": "off"
  },
  "extends": ["eslint:recommended", "google"]
}
